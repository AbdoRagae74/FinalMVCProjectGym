@model GymManagementSystem.Models.DietPlan
@{
    ViewData["Title"] = "Diet Plan";
}

<h3>Diet Plan for @ViewBag.TraineeName</h3>

<div class="card">
    <div class="card-body">
        <h4>@Model.Title</h4>
        <p>@Model.Description</p>
        <p><strong>Created At:</strong> @Model.CreatedAt?.ToString("yyyy-MM-dd")</p>
    </div>
</div>

