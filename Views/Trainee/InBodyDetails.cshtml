@model List<GymManagementSystem.Models.InBodyTest>
@{
    ViewData["Title"] = "InBody Results";
}

<h3>InBody Results for @ViewBag.TraineeName</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <th>Height</th>
            <th>Weight</th>
            <th>Fats</th>
            <th>Muscle Mass</th>
            <th>Notes</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var test in Model)
        {
            <tr>
                <td>@test.Date</td>
                <td>@test.Height</td>
                <td>@test.Weight</td>
                <td>@test.Fats</td>
                <td>@test.MuscleMass</td>
                <td>@test.Notes</td>
            </tr>
        }
    </tbody>
</table>
{
}
